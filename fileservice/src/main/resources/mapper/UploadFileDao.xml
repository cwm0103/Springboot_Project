<insert id="adduploadfile" parameterType="com.filemanage.entity.FileUpload" useGeneratedKeys="true" keyProperty="file_id" >
    INSERT  INTO file_upload(file_name,file_save_name,file_type,file_size,file_create_time,file_path,category) VALUES (
    #{file_name},#{file_save_name},#{file_type},#{file_size},#{file_create_time},#{file_path},#{category})
</insert>

    <delete id="deleteuploadfile">
        DELETE FROM file_upload WHERE file_id=#{fileid}
    </delete>

    <select id="getfiles" resultType="com.filemanage.entity.FileUpload">
        select * from file_upload WHERE file_id in
        <foreach item="item" index="index" collection="list" open="("
                 separator="," close=")">
            #{item}
        </foreach>

    </select>

    <select id="getFileUpload" resultType="com.filemanage.entity.FileUpload">
        SELECT * from file_upload WHERE file_id=#{fileid}
    </select>

    <select id="getList" resultType="com.filemanage.entity.FileUpload">
        SELECT * FROM file_upload
    </select>
